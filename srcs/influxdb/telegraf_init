sed -i "s|# skip_database_creation = false|skip_database_creation = false|g" /telegraf/etc/telegraf/telegraf.conf
sed -i "s|# username = \"telegraf\"|username = \"telegraf\"|g" /telegraf/etc/telegraf/telegraf.conf
sed -i "s|# password = \"metricsmetricsmetricsmetrics\"|password = \"telegraf\"|g" /telegraf/etc/telegraf/telegraf.conf
sed -i "s|# dc = \"us-east-1\" # will tag all metrics with dc=us-east-1| dc = \"influxdb\"|g" /telegraf/etc/telegraf/telegraf.conf